# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# AI Service Configuration
OPENAI_API_KEY=your-openai-api-key
CLAUDE_API_KEY=your-claude-api-key

# Tavus Configuration
TAVUS_API_KEY=your-tavus-api-key
TAVUS_WEBHOOK_SECRET=your-tavus-webhook-secret

# Kiro Configuration
KIRO_API_KEY=your-kiro-api-key
KIRO_API_URL=https://api.kiro.dev
KIRO_WEBHOOK_SECRET=your-kiro-webhook-secret

# Security Configuration
JWT_SECRET=your-jwt-secret
ADMIN_TOKEN=your-admin-token
ENCRYPTION_KEY=your-encryption-key

# Rate Limiting Configuration
RATE_LIMIT_ENABLED=true
MAX_REQUESTS_PER_MINUTE=100
MAX_REQUESTS_PER_HOUR=1000

# Pipeline Configuration
PIPELINE_ENABLED=true
MAX_CONCURRENT_BUILDS=5
AI_ANALYSIS_TIMEOUT=300
KIRO_API_TIMEOUT=60
RETRY_DELAYS=[1000,2000,5000,10000]
WEBHOOK_SIGNATURE_VALIDATION=true

# Monitoring Configuration
LOG_LEVEL=INFO
PERFORMANCE_MONITORING=true
ERROR_TRACKING=true
METRICS_COLLECTION=true

# Feature Flags
REALTIME_ENABLED=true
WEBSOCKET_MAX_CONNECTIONS=1000
NOTIFICATION_RETENTION_DAYS=30
REALTIME_EVENT_RETENTION_HOURS=24
BROADCAST_EVENT_RETENTION_DAYS=7
MAX_BUILD_RETRIES=3

# External Service URLs
WEBHOOK_BASE_URL=https://your-domain.com/functions/v1
FRONTEND_URL=https://your-frontend.com
DASHBOARD_URL=https://your-dashboard.com

# Database Configuration
DB_POOL_SIZE=10
DB_CONNECTION_TIMEOUT=30000
DB_QUERY_TIMEOUT=60000

# Cache Configuration
CACHE_TTL=3600
CACHE_MAX_SIZE=1000
REDIS_URL=redis://localhost:6379